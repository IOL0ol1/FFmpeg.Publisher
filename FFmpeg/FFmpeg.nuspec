<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
	<metadata>
		<id>$id</id>
		<version>$version</version>
		<authors>FFmpeg</authors>
		<owners>IOL0ol1</owners>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<license type="expression">$license</license>
		<licenseUrl>https://licenses.nuget.org/$license</licenseUrl>
		<projectUrl>https://ffmpeg.org/</projectUrl>
		<repository type="git" url="https://github.com/IOL0ol1/FFmpeg.Publisher.git"/>
		<icon>favicon.png</icon>
		<description>Headers, import libraries, and runtime dlls for FFmpeg, based on the official FFmpeg windows builds.</description>
		<summary>Headers, import libraries, and runtime dlls for FFmpeg.</summary>
		<tags>ffmpeg media video audio capture encode decode encoding decoding codec native nativepackage</tags>
		<readme>README.md</readme>
		<dependencies>
			<group targetFramework="netstandard2.1"></group>
			<group targetFramework="netstandard2.0"></group>
		</dependencies>
	</metadata>
	<files>
		<file src=".\README.md" target=".\"/>
		<file src=".\favicon.png" target="favicon.png"/>
		<file src=".\bin\*.dll" target="build\native\bin"/>
		<file src=".\doc\**" target="build\native\doc"/>
		<file src=".\include\**" target="build\native\include"/>
		<file src=".\lib\*.lib" target="build\native\lib"/>
		<file src=".\LICENSE.txt" target="build\native\LICENSE.txt"/>
		<file src=".\$id.targets" target="build\native\$id.targets"/>
		<file src=".\$id.props" target="build\netstandard\$id.props" />
		<file src=".\bin\*.dll" target="runtimes\win-x64\native"/>
		<file src=".\README.md" target="lib\netstandard2.0\_._"/>
		<file src=".\README.md" target="lib\netstandard2.1\_._"/>
	</files>
</package>
